{
    "version": 2,
    "name": "tuto",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "server.js",
        "use": "@vercel/should-serve",
        "config": {
          "packages": ["unixodbc", "unixodbc-dev"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  