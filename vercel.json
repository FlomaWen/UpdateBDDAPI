{
    "version": 2,
    "name": "tuto",
    "builds": [
      { "src": "package.json", "use": "@vercel/node" },
      {
        "use": "@vercel/should-serve",
        "config": {
          "packages": ["unixodbc", "unixodbc-dev"]
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ]
  }
  